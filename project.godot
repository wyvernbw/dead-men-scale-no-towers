config_version=5

[application]

config/name="dead-men-scale-no-towers"
run/main_scene="res://world/tower.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://icon.svg"

[autoload]

Events="*res://share/events.gd"
Tracer="*res://addons/tracer/./tracer.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
Bgm="*res://share/bgm.tscn"
__InkRuntime="*res://addons/inkgd/ink_runtime.gd"
UILayer="*res://components/gui/ui_layer.tscn"
VfxLayer="*res://components/vfx_layer.tscn"
SceneTransition="*res://components/gui/scene_transition.tscn"

[debug]

shapes/collision/shape_color=Color(0, 0.63, 0.441, 0)

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/better-terrain/plugin.cfg", "res://addons/gut/plugin.cfg", "res://addons/inkgd/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/tracer/plugin.cfg")

